    "@dnd-kit/utilities": "^3.2.2",
    "@reduxjs/toolkit": "^2.2.1",
    "antd": "^5.15.2",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@commitlint/cli": "^19.1.0",
